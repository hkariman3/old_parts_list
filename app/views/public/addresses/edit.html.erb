<%= render "public/public_header" %>

<%= form_with model: @address, url: address_path, method: :patch do |f| %>

<%= f.label :address, "住所" %>
<%= f.text_field :address %>

<%= f.label :postal_code, "郵便番号" %>
<%= f.text_field :postal_code %>

<%= f.label :telephone_number, "電話番号" %>
<%= f.text_field :telephone_number %>

<%= f.label :name, "宛名" %>
<%= f.text_field :name %>

<%= f.submit "変更する" %>

<% end %>